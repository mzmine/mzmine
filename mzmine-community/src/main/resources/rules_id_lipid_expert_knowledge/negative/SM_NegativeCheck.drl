rule "Intensity [M+CH3COO]- < [M+CH3COO+(CH3COONa)2]-"
when
    $adduct1: FoundAdduct(adductName == "[M+CH3COO+(CH3COONa)2]-")
    $adduct2: FoundAdduct(adductName == "[M+CH3COO]-")
