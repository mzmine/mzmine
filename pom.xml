		</repository>
		<repository>
			<id>genesis-maven-repository</id>
			<name>Genesis repository</name>
			<url>http://genesis.UGent.be/maven2/</url>
			<layout>default</layout>
		</repository>
		<repository>
			<id>lib</id>
			<name>MZmine local libraries</name>
			<url>file://${project.basedir}/src/main/lib</url>
